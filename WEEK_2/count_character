def count_char(word):
    dic_count_char = {}
    word = word.lower()
    for ch in word:
        if ch in dic_count_char:
            dic_count_char[ch] += 1
        else:
            dic_count_char[ch] = 1
    return dic_count_char


def main():
    print(count_char("hello"))


if __name__ == "__main__":
    main()
